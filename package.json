{
    "name": "zk-blue-tick-main-service",
    "version": "0.0.1",
    "description": "",
    "author": "HoanVh",
    "license": "MIT",
    "engines": {
        "node": ">=16.18.0 <17.0.0"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "copy:packagejson": "cpx './package.json' 'dist'",
        "copy:cert": "cpx './src/configs/cert/**' 'dist/configs/cert'",
        "build": "nest build && npm run copy:packagejson && npm run copy:cert",
        "format": "prettier --write \"src/**/*.ts\" && :",
        "start": "nest start",
        "start:dev": "npm run copy:packagejson && nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"src/**/*.ts\" --fix && :",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "pre-commit": "lint-staged",
        "precommit": "lint-staged",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "lint-staged": {
        "*": [
            "npm run format",
            "npm run lint",
            "git add"
        ]
    },
    "dependencies": {
        "@ethersproject/units": "5.7.0",
        "@metamask/eth-sig-util": "5.0.0",
        "@nestjs/axios": "1.0.0",
        "@nestjs/cli": "9.1.5",
        "@nestjs/common": "9.1.6",
        "@nestjs/core": "9.1.6",
        "@nestjs/jwt": "9.0.0",
        "@nestjs/mongoose": "9.2.1",
        "@nestjs/platform-express": "9.1.6",
        "@nestjs/schedule": "2.1.0",
        "@nestjs/terminus": "9.1.2",
        "@nestjs/throttler": "3.1.0",
        "@sendgrid/mail": "7.7.0",
        "@sentry/node": "7.26.0",
        "@trivago/prettier-plugin-sort-imports": "3.4.0",
        "@types/aws-sdk": "2.7.0",
        "@types/cache-manager": "4.0.2",
        "@types/mongodb": "4.0.7",
        "@types/multer": "1.4.7",
        "apollo-server-express": "3.11.1",
        "aws-sdk": "2.1246.0",
        "cache-manager": "5.1.3",
        "cache-manager-redis-store": "3.0.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "cpx": "1.5.0",
        "dotenv": "16.0.3",
        "ethers": "5.7.2",
        "jsdom": "20.0.3",
        "merkletreejs": "0.3.8",
        "mongoose": "6.7.1",
        "moralis": "2.10.3",
        "nestjs-i18n": "9.2.2",
        "node-base64-image": "2.0.4",
        "redis": "4.4.0",
        "rimraf": "3.0.2",
        "rxjs": "7.5.7",
        "sharp": "0.31.1",
        "ucan-storage-commonjs": "1.3.1",
        "uuid": "9.0.0",
        "voucher-code-generator": "1.3.0",
        "web3": "1.8.0"
    },
    "devDependencies": {
        "@nestjs/schematics": "9.0.3",
        "@nestjs/testing": "9.1.6",
        "@types/bull": "4.10.0",
        "@types/express": "4.17.13",
        "@types/jest": "29.2.1",
        "@types/node": "18.11.9",
        "@types/sharp": "0.31.0",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.42.0",
        "@typescript-eslint/parser": "5.42.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-unused-imports": "2.0.0",
        "jest": "29.2.2",
        "lint-staged": "13.1.0",
        "prettier": "2.7.1",
        "supertest": "6.3.1",
        "ts-loader": "9.4.1",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.1.0",
        "typescript": "4.8.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
